{
  "name": "nest-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "dprint fmt",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate": "migrate-mongo",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "test:unit": "jest --config test/jest-unit.json",
    "test:integration": "jest --config test/jest-integration.json --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "3.363.0",
    "@aws-sdk/client-sns": "3.363.0",
    "@m8a/nestjs-typegoose": "^12.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "10.0.6",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "@sniptt/monads": "^0.5.10",
    "@typegoose/typegoose": "12.3.1",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.2.3",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "hibp": "^13.0.0",
    "ioredis": "^5.3.2",
    "lodash": "4.17.21",
    "module-alias": "^2.2.3",
    "mongoose": "8.2.4",
    "mongoose-lean-virtuals": "^0.9.1",
    "ms": "^2.1.3",
    "nanoid": "3.3.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqs-consumer": "7.2.1",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/jest-when": "^3.5.2",
    "@types/lodash": "^4.14.196",
    "@types/ms": "^0.7.31",
    "@types/nanoid": "^3.0.0",
    "@types/node": "20.4.5",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dprint": "^0.45.0",
    "eslint": "^8.46.0",
    "eslint-plugin-dprint-integration": "^0.3.0",
    "jest": "29.6.2",
    "jest-extended": "^4.0.0",
    "jest-when": "^3.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@aws-sdk/client-sqs": "3.363.0"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    },
    "rootDir": "./",
    "testMatch": [
      "<rootDir>/test/unit/**/*.spec.ts",
      "<rootDir>/test/integration/**/*.integration-spec.ts",
      "<rootDir>/test/e2e/**/*.e2e-spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/test.setup.ts"
    ],
    "resetMocks": true,
    "restoreMocks": true
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
